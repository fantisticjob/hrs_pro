package com.hausontech.hrs.bean.allocationManager.model;

import java.util.Date;

import com.fasterxml.jackson.databind.annotation.JsonSerialize;
import com.hausontech.hrs.bean.AuditBean;
import com.hausontech.hrs.bean.JsonDateSerializer;

public class AllocRuleRecord extends AuditBean implements java.io.Serializable {
	private long allocRuleId;
	private int lineNumber;
	private String ruleName;
	private String driverType;
	private Date startDateActive;
	private Date endDateActive;
	private String description;
	private int sourceId;
	private int driverId;
	private int targetId;
	private int offsetId;
	private String isSourceOver;
	private String isDriverOver;
	public long getAllocRuleId() {
		return allocRuleId;
	}
	public void setAllocRuleId(long allocRuleId) {
		this.allocRuleId = allocRuleId;
	}
	public int getLineNumber() {
		return lineNumber;
	}
	public void setLineNumber(int lineNumber) {
		this.lineNumber = lineNumber;
	}
	public String getRuleName() {
		return ruleName;
	}
	public void setRuleName(String ruleName) {
		this.ruleName = ruleName;
	}
	public String getDriverType() {
		return driverType;
	}
	public void setDriverType(String driverType) {
		this.driverType = driverType;
	}
	@JsonSerialize(using = JsonDateSerializer.class)
	public Date getStartDateActive() {
		return startDateActive;
	}
	public void setStartDateActive(Date startDateActive) {
		this.startDateActive = startDateActive;
	}
	@JsonSerialize(using = JsonDateSerializer.class)
	public Date getEndDateActive() {
		return endDateActive;
	}
	public void setEndDateActive(Date endDateActive) {
		this.endDateActive = endDateActive;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public int getSourceId() {
		return sourceId;
	}
	public void setSourceId(int sourceId) {
		this.sourceId = sourceId;
	}
	public int getDriverId() {
		return driverId;
	}
	public void setDriverId(int driverId) {
		this.driverId = driverId;
	}
	public int getTargetId() {
		return targetId;
	}
	public void setTargetId(int targetId) {
		this.targetId = targetId;
	}
	public int getOffsetId() {
		return offsetId;
	}
	public void setOffsetId(int offsetId) {
		this.offsetId = offsetId;
	}
	public String getIsSourceOver() {
		return isSourceOver;
	}
	public void setIsSourceOver(String isSourceOver) {
		this.isSourceOver = isSourceOver;
	}
	public String getIsDriverOver() {
		return isDriverOver;
	}
	public void setIsDriverOver(String isDriverOver) {
		this.isDriverOver = isDriverOver;
	}
	
	
	

}
