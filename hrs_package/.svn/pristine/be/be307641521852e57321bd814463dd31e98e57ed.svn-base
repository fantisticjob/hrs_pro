					&& this.isEqual(this.segment3, otherCalBean.getSegment3()) && this.isEqual(this.segment4, otherCalBean.getSegment4()) && this.isEqual(this.segment5, otherCalBean.getSegment5())
					&& this.isEqual(this.segment6, otherCalBean.getSegment6()) && this.isEqual(this.segment7, otherCalBean.getSegment7()) && this.isEqual(this.segment8, otherCalBean.getSegment8())
