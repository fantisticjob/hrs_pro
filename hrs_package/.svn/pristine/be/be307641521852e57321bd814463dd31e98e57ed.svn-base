					&& this.isEqual(this.segment13, otherCalBean.getSegment13()) && this.isEqual(this.segment14, otherCalBean.getSegment14())
					&& this.isEqual(this.segment15, otherCalBean.getSegment15()) && this.isEqual(this.segment16, otherCalBean.getSegment16())
					&& this.isEqual(this.segment17, otherCalBean.getSegment17()) && this.isEqual(this.segment18, otherCalBean.getSegment18())
					&& this.isEqual(this.segment19, otherCalBean.getSegment19()) && this.isEqual(this.segment20, otherCalBean.getSegment20())) {
