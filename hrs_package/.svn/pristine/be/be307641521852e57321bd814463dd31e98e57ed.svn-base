			} else {
				return false;
			}
		} else {
			return false;
		}
	}
	
	
	public boolean isContantBelongSameGroup(CalculationProcessorBean contentRecord) {
		boolean result = false;
		if (contentRecord != null && this.isEqual(this.ruleCode, contentRecord.getRuleCode()) && this.isEqual(this.itemCode, contentRecord.getItemCode()) && isEqual(this.finElement,contentRecord.getFinElement())
				&& this.ledgerId == contentRecord.getLedgerId() && this.isEqual(this.actualFlag,contentRecord.getActualFlag())
				&& this.isEqual(this.periodName, contentRecord.getPeriodName())) {
