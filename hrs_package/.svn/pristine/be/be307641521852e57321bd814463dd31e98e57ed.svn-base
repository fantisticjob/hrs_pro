	private boolean isEqual(String source, String target) {
		if (!StringUtils.isBlank(source)) {
			if (!StringUtils.isBlank(target) && source.equals(target)) {
				return true;
			} else {
				return false;
			}
		} else {
			if (StringUtils.isBlank(target)) {
				return true;
			} else {
				return false;
			}
		}
	}
	
	public boolean equals(Object obj) {
