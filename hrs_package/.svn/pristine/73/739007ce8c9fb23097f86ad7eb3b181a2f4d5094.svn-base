package com.hausontech.hrs.bean.reportSetting;

public class DimValueNamePair {
    private long dimensionId; 
	private String dimValueCode;
	private String dimValueName;
	
	public DimValueNamePair(String code, String name) {
		this.dimValueCode = code;
		this.dimValueName = name;
	}
	
	public DimValueNamePair(long dimId, String code, String name) {
		this.dimensionId = dimId;
		this.dimValueCode = code;
		this.dimValueName = name;
	}
	
	public DimValueNamePair() {}
	
	public String getDimValueCode() {
		return dimValueCode;
	}
	public void setDimValueCode(String dimValueCode) {
		this.dimValueCode = dimValueCode;
	}
	public String getDimValueName() {
		return dimValueName;
	}
	public void setDimValueName(String dimValueName) {
		this.dimValueName = dimValueName;
	}

	public long getDimensionId() {
		return dimensionId;
	}

	public void setDimensionId(long dimensionId) {
		this.dimensionId = dimensionId;
	}
	
	
	
	
	

}
