				</if>
				<if test="itemDescription != null and itemDescription != ''">
					and a. DESCRIPTION like '%' || #{itemDescription} || '%'
				</if>
			</otherwise>
		</choose>
	</sql>

	<!-- 查询表项记录 -->
	<select id="getItemHeaderList"
		parameterType="com.hausontech.hrs.bean.reportSetting.ItemHeaderBean"
		resultMap="itemHeaderResultMap">
		<include refid="pageBefore"/>
		SELECT ITEM_HEADER_ID, ITEM_CODE, DESCRIPTION, RULE_CODE ,
		CREATION_DATE, CREATED_BY, LAST_UPDATE_DATE, LAST_UPDATED_BY,
		<if test="rowStartIndex != 0 and rowCount != 0">
			ROW_NUMBER() OVER (ORDER BY 1) AS row_number ,
		</if>
		CASE (SELECT COUNT(1) FROM HRS_DEF_ITEM_CONTENT b WHERE a.item_code =
		b.item_code)
		WHEN 0 THEN 'N' ELSE 'Y' END IS_CONTENT,
		CASE (SELECT COUNT(1) FROM HRS_DEF_ITEM_CALCULATION c WHERE c.item_code =
		a.item_code)
		WHEN 0 THEN 'N' ELSE 'Y' END IS_CALCULATION
		FROM HRS_DEF_ITEM_HEADER a
		<include refid="itemHeaderCondition"/>
		<include refid="pageAfter"/>
	</select>
	
	<!-- 查询循环组记录条数-->
	<select id="countByItemGroupRuleHeaderBean" parameterType="com.hausontech.hrs.bean.reportSetting.ItemHeaderBean" resultType="java.lang.Integer">
		select count(1) from HRS_DEF_ITEM_HEADER 
		<include refid="itemHeaderCondition"/>
	</select>
	
	
	<!-- 查询循环组记录条数(行集表)-->
	<select id="countByItemGroupRuleLineBean" parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleLineBean" resultType="java.lang.Integer">
		select count(1) from HRS_DEF_RULE_LINE 
		<include refid="itemGroupRuleHeaderCondition"/>
	</select>
	
	
	<!-- 插入循环组记录 -->
	<insert id="saveRuleHeader" parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleHeaderBean">
		INSERT INTO HRS_DEF_RULE_HEADER(RULE_HEADER_ID, RULE_CODE, DESCRIPTION
		<include refid="AUDIT_QUERY_STRING" />
		)
		VALUES(#{ruleHeaderId}, #{ruleCode}, #{description}
		<include refid="AUDIT_QUERY_STRING_L" />
		)
	</insert>
	
	<!-- 更新循环组数据 -->
	<update id="updateRuleHeader" parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleHeaderBean">
		 UPDATE HRS_DEF_RULE_HEADER SET
		 RULE_CODE =NVL(#{ruleCode},RULE_CODE),
		 DESCRIPTION =NVL(#{description},DESCRIPTION),
		 LAST_UPDATE_DATE =NVL(#{lastUpdateDate},LAST_UPDATE_DATE),
		 LAST_UPDATED_BY =NVL(#{lastUpdatedBy},LAST_UPDATED_BY)
		 WHERE RULE_HEADER_ID=#{ruleHeaderId}
	</update>
	
	<!-- 根据主键查询循环组 -->
	<select id="getRuleHeaderByByPrimaryKey" parameterType="java.lang.Integer"
		resultMap="itemGroupRuleHeaderResultMap">
		SELECT RULE_HEADER_ID, RULE_CODE, DESCRIPTION, CREATION_DATE, CREATED_BY, LAST_UPDATE_DATE, LAST_UPDATED_BY
		FROM HRS_DEF_RULE_HEADER WHERE RULE_HEADER_ID=#{primaryKey}
	</select>
	
	<!-- 查询外部代码定义记录条数 -->
	<select id="getRuleHeaderReferencedNum"
		parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleHeaderBean"
		resultType="java.lang.Integer">
		select count(1) from HRS_DEF_ITEM_HEADER where RULE_CODE=#{ruleCode}
	</select>
	
	<!-- 删除循环组-->
	<delete id="deleteReferencedRuleLine"
		parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleHeaderBean">
		DELETE HRS_DEF_RULE_LINE WHERE RULE_HEADER_ID=#{ruleHeaderId}
	</delete>
	
	
	<!-- 删除循环组（表项） -->
	<delete id="deleteRuleHeader"
		parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleHeaderBean">
		DELETE HRS_DEF_RULE_HEADER WHERE RULE_HEADER_ID=#{ruleHeaderId}
	</delete>
	
	
	<!-- 查询循环组信息  该方法返回的类型是 List<Map<String, Object>> -->
	<select id="getRuleLineList" resultType="java.util.Map">
		SELECT t.* , d.dimension_name DESCRIPTION , 
		ROW_NUMBER() OVER (ORDER BY 1) AS row_number
		FROM HRS_DEF_RULE_LINE t, HRS_CORE_DIMENSION d
		WHERE t.dim_segment = d.dim_segment
		<include refid="itemGroupRuleHeaderCondition1"/>
	</select>
	
	
	<!-- 插入循环组记录 (表项)-->
	<insert id="saveRuleLineRecord" parameterType="com.hausontech.hrs.bean.reportSetting.ItemGroupRuleLineBean">
		INSERT INTO HRS_DEF_RULE_LINE(RULE_LINE_ID, RULE_LINE_SEQ, RULE_HEADER_ID, DIM_SEGMENT
		<include refid="AUDIT_QUERY_STRING" />
		)
		VALUES(#{ruleLineId}, #{ruleLineSeq}, #{ruleHeaderId}, #{dimSegment}
		<include refid="AUDIT_QUERY_STRING_L" />
		)
	</insert>
	
	
</mapper>