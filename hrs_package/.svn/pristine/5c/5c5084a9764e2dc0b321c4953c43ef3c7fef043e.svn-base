package com.hausontech.hrs.bean.allocationManager;

import java.util.Date;

import com.hausontech.hrs.bean.AuditBean;

public class DriverStaticLine extends AuditBean {

	public int getStaticLineId() {
		return staticLineId;
	}
	public void setStaticLineId(int staticLineId) {
		this.staticLineId = staticLineId;
	}
	public int getStaticHeaderId() {
		return staticHeaderId;
	}
	public void setStaticHeaderId(int staticHeaderId) {
		this.staticHeaderId = staticHeaderId;
	}
	public String getDimValue() {
		return dimValue;
	}
	public void setDimValue(String dimValue) {
		this.dimValue = dimValue;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public Double getProportion() {
		return proportion;
	}
	public void setProportion(Double proportion) {
		this.proportion = proportion;
	}
	public Date getStartDateActive() {
		return startDateActive;
	}
	public void setStartDateActive(Date startDateActive) {
		this.startDateActive = startDateActive;
	}
	public Date getEndDateActive() {
		return endDateActive;
	}
	public void setEndDateActive(Date endDateActive) {
		this.endDateActive = endDateActive;
	}
    public String getDriverName() {
		return driverName;
	}
	public void setDriverName(String driverName) {
		this.driverName = driverName;
	}
	private int staticLineId;
	private int staticHeaderId;
	private String dimValue;
	private String driverName;
	private String description;
    private Double proportion;
	private Date startDateActive;
	private Date endDateActive;
    
	

}
