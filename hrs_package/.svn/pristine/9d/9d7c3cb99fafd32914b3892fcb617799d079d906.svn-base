package com.hausontech.hrs.bean.allocationManager;

import com.hausontech.hrs.bean.AuditBean;

public class AllocTargetAccountBean extends AuditBean{
	private int targetAccId;
	private int targetId;
	private int ledgerId;
	private String finElement;
	private String dimensionSegment;
	private String dimAllocType;
	private String dimensionValue;
	private String description;
	public int getTargetAccId() {
		return targetAccId;
	}
	public void setTargetAccId(int targetAccId) {
		this.targetAccId = targetAccId;
	}
	public int getTargetId() {
		return targetId;
	}
	public void setTargetId(int targetId) {
		this.targetId = targetId;
	}
	public int getLedgerId() {
		return ledgerId;
	}
	public void setLedgerId(int ledgerId) {
		this.ledgerId = ledgerId;
	}
	public String getFinElement() {
		return finElement;
	}
	public void setFinElement(String finElement) {
		this.finElement = finElement;
	}
	public String getDimensionSegment() {
		return dimensionSegment;
	}
	public void setDimensionSegment(String dimensionSegment) {
		this.dimensionSegment = dimensionSegment;
	}
	public String getDimAllocType() {
		return dimAllocType;
	}
	public void setDimAllocType(String dimAllocType) {
		this.dimAllocType = dimAllocType;
	}
	public String getDimensionValue() {
		return dimensionValue;
	}
	public void setDimensionValue(String dimensionValue) {
		this.dimensionValue = dimensionValue;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
}
